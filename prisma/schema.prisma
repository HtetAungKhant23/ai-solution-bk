generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = ""
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Author {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
